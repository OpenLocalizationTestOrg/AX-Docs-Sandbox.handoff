        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>DialogField dialogFeeLedgerDimension;</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>LedgerDimensionAccountController ledgerDimensionAccountController;</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>dialogFeeLedgerDimension = <ph id="ph1">dialog.addFieldValue(extendedtypestr(LedgerDimensionAccount),feeLedgerDimension,"@SYS119703");</ph></source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>ledgerDimensionAccountController = LedgerDimensionAccountController::constructForDialog(dialogFeeLedgerDimension);</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source><bpt id="p1">**</bpt>In Dynamics AX, this code would be converted to:<ept id="p1">**</ept></source>
