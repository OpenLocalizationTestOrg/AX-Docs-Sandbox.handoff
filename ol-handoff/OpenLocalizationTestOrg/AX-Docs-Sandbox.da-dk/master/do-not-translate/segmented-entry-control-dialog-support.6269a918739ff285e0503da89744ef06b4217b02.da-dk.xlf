        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>DialogField dialogFeeLedgerDimension;</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>dialogFeeLedgerDimension = SegmentedEntryControlBuild::addToDialog(dialog, classstr(LedgerDimensionAccountController), extendedTypeStr(LedgerDimensionAccount), <ph id="ph1">"@SYS119703",</ph> feeLedgerDimension);</source>
        </trans-unit>
