        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>You can also control how individual reports, the report library, and the default file locations are displayed.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Set user preferences</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>In Report Designer, on the <bpt id="p1">**</bpt>Tools<ept id="p1">**</ept> menu, click <bpt id="p2">**</bpt>Options<ept id="p2">**</ept>.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>In the <bpt id="p1">**</bpt>Options<ept id="p1">**</ept> dialog box, select your preferences.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Click <bpt id="p1">**</bpt>OK<ept id="p1">**</ept>.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Options dialog box options</source>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>User option</source>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>Description</source>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>At startup</source>
        </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>Select what you see when Management Reporters starts.</source>
        </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>Select one of the following options:</source>
        </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source><bpt id="p1">&lt;strong&gt;</bpt>Load last loaded report<ept id="p1">&lt;/strong&gt;</ept> – Open the last report that you viewed.</source>
        </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve">
          <source><bpt id="p1">&lt;strong&gt;</bpt>Show Open window<ept id="p1">&lt;/strong&gt;</ept> – Open the <bpt id="p2">&lt;strong&gt;</bpt>Open<ept id="p2">&lt;/strong&gt;</ept> window.</source>
        </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve">
          <source>The <bpt id="p1">&lt;strong&gt;</bpt>Open<ept id="p1">&lt;/strong&gt;</ept> window provides a search window that you can use to quickly find building blocks.</source>
        </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve">
          <source><bpt id="p1">&lt;strong&gt;</bpt>Show empty environment<ept id="p1">&lt;/strong&gt;</ept> – Open an empty designer window.</source>
