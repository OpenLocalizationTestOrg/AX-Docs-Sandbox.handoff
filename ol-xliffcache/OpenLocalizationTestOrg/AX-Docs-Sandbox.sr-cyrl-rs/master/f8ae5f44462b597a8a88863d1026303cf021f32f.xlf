        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>Display confirmation before exiting Management Reporter</source>
        </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>Select whether to receive a warning message when you exit the application.</source>
