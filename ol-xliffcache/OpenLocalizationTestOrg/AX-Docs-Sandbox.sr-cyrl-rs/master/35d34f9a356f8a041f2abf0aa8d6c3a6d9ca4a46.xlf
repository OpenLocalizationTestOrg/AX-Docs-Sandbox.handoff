        <trans-unit id="190" translate="yes" xml:space="preserve">
          <source>The following table shows the key aggregate measurements of the invoice lines entity that are used to create several calculated measures in the content pack’s dataset.</source>
        </trans-unit>
        <trans-unit id="191" translate="yes" xml:space="preserve">
          <source><bpt id="p1">**</bpt>Measure<ept id="p1">**</ept></source>
        </trans-unit>
        <trans-unit id="192" translate="yes" xml:space="preserve">
          <source><bpt id="p1">**</bpt>Calculated as<ept id="p1">**</ept></source>
        </trans-unit>
        <trans-unit id="193" translate="yes" xml:space="preserve">
          <source>Gross profit</source>
        </trans-unit>
        <trans-unit id="194" translate="yes" xml:space="preserve">
          <source>SUM(Revenue – COGS – Commission – Sales tax (included in customer invoice line amount))</source>
        </trans-unit>
        <trans-unit id="195" translate="yes" xml:space="preserve">
          <source>Gross margin</source>
        </trans-unit>
        <trans-unit id="196" translate="yes" xml:space="preserve">
          <source>SUM(Gross profit / (Revenue - Sales tax (included in customer invoice line amount)))</source>
        </trans-unit>
        <trans-unit id="197" translate="yes" xml:space="preserve">
          <source>Revenue last year</source>
        </trans-unit>
        <trans-unit id="198" translate="yes" xml:space="preserve">
          <source>Revenue last year = CALCULATE(SUM('Invoice lines'<ph id="ph1">\[</ph>Revenue<ph id="ph2">\]</ph>), SAMEPERIODLASTYEAR(Dates<ph id="ph3">\[</ph>Date<ph id="ph4">\]</ph>)</source>
        </trans-unit>
