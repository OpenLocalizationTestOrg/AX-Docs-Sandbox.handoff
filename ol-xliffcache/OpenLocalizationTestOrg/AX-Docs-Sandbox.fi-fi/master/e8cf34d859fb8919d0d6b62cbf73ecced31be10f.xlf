        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>To import the Chinese Golden Tax data entity, complete the following steps.</source>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Go to <bpt id="p1">**</bpt>System administration<ept id="p1">**</ept> <ph id="ph1">&amp;gt;</ph> <bpt id="p2">**</bpt>Data management<ept id="p2">**</ept>.</source>
