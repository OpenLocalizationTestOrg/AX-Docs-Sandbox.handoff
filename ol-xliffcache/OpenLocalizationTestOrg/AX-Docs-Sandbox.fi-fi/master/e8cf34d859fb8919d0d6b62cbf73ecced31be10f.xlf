        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Click the <bpt id="p1">**</bpt>Import<ept id="p1">**</ept> tile to create a new import project.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Enter a name for the project.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Select the source data format <bpt id="p1">**</bpt>XML-Element<ept id="p1">**</ept>.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>In the <bpt id="p1">**</bpt>Entity name<ept id="p1">**</ept> field, select <bpt id="p2">**</bpt>Tax integration import<ept id="p2">**</ept> entity.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Click upload and then browse to the location of your golden tax data file.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Click <bpt id="p1">**</bpt>View map<ept id="p1">**</ept> on the <bpt id="p2">**</bpt>Tax integration import<ept id="p2">**</ept> tile.</source>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>On the <bpt id="p1">**</bpt>Transformations<ept id="p1">**</ept> tab, click <bpt id="p2">**</bpt>New<ept id="p2">**</ept>.</source>
